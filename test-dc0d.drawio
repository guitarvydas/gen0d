        <mxCell id="LY3EbIqGwphYwSvvPRLA-22" value="p.pwr += weapon.pwr&lt;br&gt;p.wallet -= weapon.const&lt;br&gt;send (&quot;&quot;, bang)" style="rounded=0;whiteSpace=wrap;html=1;dashed=1;fillColor=#f9f7ed;strokeColor=#36393d;align=left;verticalAlign=top;" vertex="1" parent="1">
