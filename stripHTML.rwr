stripHTML {
  chars [char+] = ‛«char»’

  char_italicWithAttr [lb nonRB+ rb] = ‛’
  char_italicNoAttr [_] = ‛’
  char_closeItalic [_] = ‛’

  char_boldWithAttr [lb nonRB+ rb] = ‛’
  char_boldNoAttr [_] = ‛’
  char_closeBold [_] = ‛’

  char_spanWithAttr [lb nonRB+ rb] = ‛’
  char_spanNoAttr [_] = ‛’
  char_closeSpan [_] = ‛’

  char_brWithAttr [lb nonRB+ rb] = ‛<br>’
  char_brNoAttr [_] = ‛<br>’
  char_closeBr [_] = ‛’

  char_other [c] = ‛«c»’

  nonRB [c] = ‛«c»’
}
